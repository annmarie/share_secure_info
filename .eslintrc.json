{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "next/core-web-vitals"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "plugins": ["prettier"],
    "rules": {
      "linebreak-style": ["error", "unix"],
      "prettier/prettier": "error"
    }
  }
}
